{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "",
  "projects": {
    "sandbox": {
      "root": "apps/sandbox/",
      "sourceRoot": "apps/sandbox/src",
      "projectType": "application",
      "prefix": "hlc",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/apps/sandbox",
            "index": "apps/sandbox/src/index.html",
            "main": "apps/sandbox/src/main.ts",
            "polyfills": "apps/sandbox/src/polyfills.ts",
            "tsConfig": "apps/sandbox/tsconfig.app.json",
            "assets": [
              "apps/sandbox/src/favicon.ico",
              "apps/sandbox/src/assets"
            ],
            "styles": [
              "apps/sandbox/src/styles.scss",
              "node_modules/@clr/ui/clr-ui.min.css",
              "node_modules/@clr/icons/clr-icons.min.css"
            ],
            "scripts": [
              "node_modules/@webcomponents/custom-elements/custom-elements.min.js",
              "node_modules/@clr/icons/clr-icons.min.js"
            ]
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "apps/sandbox/src/environments/environment.ts",
                  "with": "apps/sandbox/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "sandbox:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "sandbox:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "sandbox:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "apps/sandbox/src/test.ts",
            "polyfills": "apps/sandbox/src/polyfills.ts",
            "tsConfig": "apps/sandbox/tsconfig.spec.json",
            "karmaConfig": "apps/sandbox/karma.conf.js",
            "styles": [
              "apps/sandbox/src/styles.scss"
            ],
            "scripts": [],
            "assets": [
              "apps/sandbox/src/favicon.ico",
              "apps/sandbox/src/assets"
            ]
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "apps/sandbox/tsconfig.app.json",
              "apps/sandbox/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    },
    "forms": {
      "root": "libs/forms",
      "sourceRoot": "libs/forms/src",
      "projectType": "library",
      "prefix": "hlc",
      "architect": {
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "libs/forms/src/test.ts",
            "tsConfig": "libs/forms/tsconfig.spec.json",
            "polyfills": "libs/forms/src/polyfills.ts",
            "karmaConfig": "libs/forms/karma.conf.js"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "libs/forms/tsconfig.lib.json",
              "libs/forms/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    },
    "clr-controls": {
      "root": "libs/clr-controls",
      "sourceRoot": "libs/clr-controls/src",
      "projectType": "library",
      "prefix": "hlc",
      "architect": {
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "libs/clr-controls/src/test.ts",
            "tsConfig": "libs/clr-controls/tsconfig.spec.json",
            "karmaConfig": "libs/clr-controls/karma.conf.js"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "libs/clr-controls/tsconfig.lib.json",
              "libs/clr-controls/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    },
    "clr-layout": {
      "root": "libs/clr-layout",
      "sourceRoot": "libs/clr-layout/src",
      "projectType": "library",
      "prefix": "hlc",
      "architect": {
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "libs/clr-layout/src/test.ts",
            "tsConfig": "libs/clr-layout/tsconfig.spec.json",
            "karmaConfig": "libs/clr-layout/karma.conf.js"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "libs/clr-layout/tsconfig.lib.json",
              "libs/clr-layout/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    },
    "clr-forms": {
      "root": "libs/clr-forms",
      "sourceRoot": "libs/clr-forms/src",
      "projectType": "library",
      "prefix": "ng-holistic",
      "architect": {
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "libs/clr-forms/src/test.ts",
            "tsConfig": "libs/clr-forms/tsconfig.spec.json",
            "karmaConfig": "libs/clr-forms/karma.conf.js"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "libs/clr-forms/tsconfig.lib.json",
              "libs/clr-forms/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    }
  },
  "cli": {
    "warnings": {
      "typescriptMismatch": false,
      "versionMismatch": false
    },
    "defaultCollection": "@nrwl/schematics"
  },
  "defaultProject": "sandbox",
  "schematics": {
    "@schematics/angular:component": {
      "styleext": "scss",
      "changeDetection": "OnPush",
      "spec": false,
      "skipImport": true,
      "prefix": "hlc"
    },
    "@nrwl/schematics:component": {
      "styleext": "scss",
      "changeDetection": "OnPush",
      "spec": false,
      "skipImport": true,
      "skipFormat": false,
      "prefix": "hlc"
    },
    "@schematics/angular:service": {
      "spec": false,
      "skipImport": true
    },
    "@nrwl/schematics:service": {
      "spec": false,
      "skipImport": true,
      "skipFormat": false
    },
    "@schematics/angular:directive": {
      "spec": false,
      "skipImport": true,
      "prefix": "hlc"
    },
    "@nrwl/schematics:directive": {
      "spec": false,
      "skipImport": true,
      "skipFormat": false,
      "prefix": "hlc"
    }
  }
}
